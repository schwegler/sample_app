<h1>New Movie</h1>
<%= form_for @movie do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :director %>
    <%= f.text_field :director %>
  </div>
  <div>
    <%= f.label :release_year %>
    <%= f.number_field :release_year %>
  </div>
  <div>
    <%= f.label :rating %>
    <%= f.text_field :rating %>
  </div>
  <%= f.submit %>
<% end %>
